    "shortid": "^2.2.16",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/node": "^20.4.8",
    "@types/sequelize": "^4.28.15",
